          ".groovy.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-systemd",
        "aliases": [
          "MTRGen Systemd Unit File",
          "mtr-systemd"
        ],
        "extensions": [
          ".link.mtr",
          ".netdev.mtr",
          ".network.mtr",
          ".service.mtr",
          ".socket.mtr",
          ".device.mtr",
          ".mount.mtr",
          ".automount.mtr",
          ".swap.mtr",
          ".target.mtr",
          ".path.mtr",
          ".timer.mtr",
          ".snapshot.mtr",
          ".slice.mtr",
          ".scope.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-cpp",
        "aliases": [
          "MTRGen C++",
          "mtr-cpp"
        ],
        "extensions": [
          ".c.mtr",
          ".cpp.mtr",
          ".h.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-cisco",
        "aliases": [
          "MTRGen Cisco Config"
        ],
        "extensions": [
          ".ios.mtr",
          ".cisco.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-java",
        "aliases": [
          "MTRGen Java"
        ],
        "extensions": [
          ".java.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-php",
        "aliases": [
          "MTRGen PHP"
        ],
        "extensions": [
          ".php.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-rust",
        "aliases": [
          "MTRGen Rust"
        ],
        "extensions": [
          ".rs.mtr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "htmlLanguageParticipants": [
      {
        "languageId": "mtrgen-html",
        "autoInsert": true
      }
    ],
    "grammars": [
      {
        "language": "mtrgen-html",
