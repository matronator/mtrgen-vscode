{
  "name": "mtrgen-vscode",
  "displayName": "MTRGen Templates Syntax Highlighting",
  "description": "Syntax highlighting for MTRGen including HTML, Markdown, YAML, Ruby more...",
  "version": "0.1.0",
  "publisher": "matronator",
  "author": "Matronator",
  "engines": {
    "vscode": "^1.22.0"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/matronator"
  },
  "license": "MIT",
  "icon": "icon.png",
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "homepage": "https://github.com/matronator/mtrgen-vscode",
  "keywords": [
    "mtrgen",
    "templates",
    "templating",
    "syntax",
    "highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matronator/mtrgen-vscode.git"
  },
  "main": "./src/extension.js",
  "scripts": {
    "format": "prettier --write '**/*.{js,json,md}'",
    "lint": "prettier --check '**/*.{js,json,md}'"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 119,
    "bracketSpacing": false,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "prettier": "2.7.1"
  },
  "activationEvents": [
    "onLanguage:mtrgen-html"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mtrgen-html",
        "aliases": [
          "MTRGen HTML",
          "mtr-html"
        ],
        "extensions": [
          ".html.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-xml",
        "aliases": [
          "MTRGen XML",
          "mtr-xml"
        ],
        "extensions": [
          ".xml.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-css",
        "aliases": [
          "MTRGen CSS",
          "mtr-css"
        ],
        "extensions": [
          ".css.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-json",
        "aliases": [
          "MTRGen JSON",
          "mtr-json"
        ],
        "extensions": [
          ".json.mtr",
          ".code-workspace.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-md",
        "aliases": [
          "MTRGen Markdown",
          "mtr-md"
        ],
        "extensions": [
          ".md.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-py",
        "aliases": [
          "MTRGen Python",
          "mtr-py"
        ],
        "extensions": [
          "py.mtr",
          "pyi.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-cy",
        "aliases": [
          "MTRGen Cython",
          "mtr-cy"
        ],
        "extensions": [
          "pyx.mtr",
          "pxd.mtr",
          "pxi.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-rb",
        "aliases": [
          "MTRGen Ruby",
          "mtr-rb"
        ],
        "extensions": [
          "rb.mtr",
          "rbw.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-js",
        "aliases": [
          "MTRGen JavaScript",
          "MTRGen JS",
          "mtr-js"
        ],
        "extensions": [
          "js.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-ts",
        "aliases": [
          "MTRGen TypeScript",
          "MTRGen TS",
          "mtr-ts"
        ],
        "extensions": [
          "ts.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-yaml",
        "aliases": [
          "MTRGen YAML",
          "mtr-yaml"
        ],
        "extensions": [
          ".yml.mtr",
          ".yaml.mtr",
          ".sls.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-toml",
        "aliases": [
          "MTRGen TOML",
          "mtr-toml"
        ],
        "extensions": [
          ".toml.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-latex",
        "aliases": [
          "MTRGen LaTeX",
          "mtr-latex"
        ],
        "extensions": [
          ".latex.mtr",
          ".tex.mtr"
        ],
        "configuration": "./language-configuration-latex.json"
      },
      {
        "id": "mtrgen-lua",
        "aliases": [
          "MTRGen Lua",
          "mtr-lua"
        ],
        "extensions": [
          ".lua.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-properties",
        "aliases": [
          "MTRGen Properties/ini/Conf",
          "mtr-properties",
          "mtrgen-ini",
          "mtrgen-conf"
        ],
        "extensions": [
          ".properties.mtr",
          ".cfg.mtr",
          ".conf.mtr",
          ".desktop.mtr",
          ".directory.mtr",
          ".ini.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-shell",
        "aliases": [
          "MTRGen Shell Script",
          "mtrgen-shellscript",
          "mtr-shell",
          "mtrgen-sh",
          "mtrgen-bash"
        ],
        "extensions": [
          ".sh.mtr",
          ".bash.mtr",
          ".bashrc.mtr",
          ".bash_aliases.mtr",
          ".bash_profile.mtr",
          ".bash_login.mtr",
          ".ebuild.mtr",
          ".install.mtr",
          ".profile.mtr",
          ".bash_logout.mtr",
          ".zsh.mtr",
          ".zshrc.mtr",
          ".zprofile.mtr",
          ".zlogin.mtr",
          ".zlogout.mtr",
          ".zshenv.mtr",
          ".zsh-theme.mtr",
          ".ksh.mtr",
          ".env.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-dockerfile",
        "aliases": [
          "MTRGen Dockerfile",
          "mtr-dockerfile"
        ],
        "extensions": [
          ".dockerfile.mtr"
        ],
        "filenames": [
          "Dockerfile.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-sql",
        "aliases": [
          "MTRGen SQL",
          "mtr-sql"
        ],
        "extensions": [
          ".sql.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen",
        "aliases": [
          "MTRGen Raw",
          "mtrgen-raw"
        ],
        "extensions": [],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-terraform",
        "aliases": [
          "MTRGen Terraform",
          "mtr-terraform",
          "MTRGen-HCL",
          "mtrgen-hcl",
          "MTRGen-Hashicorp",
          "mtrgen-hashicorp"
        ],
        "extensions": [
          ".tf.mtr",
          ".tfvars.mtr",
          ".hcl.mtr"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "mtrgen-nginx",
